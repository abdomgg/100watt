<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_valuation_layer_tree_inherit" model="ir.ui.view">
        <field name="name">stock.valuation.layer.tree.inherit</field>
        <field name="model">stock.valuation.layer</field>
        <field name="inherit_id" ref="stock_account.stock_valuation_layer_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="product_qty_available" string="On Hand" optional="show"/>
            </xpath>
            <xpath expr="//field[@name='value']" position="after">
                <field name="total_inventory_value" string="القيمة الكلية المخزنية" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
